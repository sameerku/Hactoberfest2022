// Using Math.log2()

var value = Math.log2(1);

console.log(value); // 0

var value = Math.log2(2);

console.log(value); // 1

var value = Math.log2("10");

console.log(value); // 3.321928094887362

var value = Math.log2(0);

console.log(value); // -Infinity

var value = Math.log2(-1);

console.log(value); // NaN
